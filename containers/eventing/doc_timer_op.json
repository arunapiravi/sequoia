{
	"appname": "doc_timer_op",
	"id": 0,
	"depcfg": {
		"buckets": [{
			"alias": "dst",
			"bucket_name": "doc_timer_dst"
		}],
		"metadata_bucket": "metadata",
		"source_bucket": "doc_timer"
	},
	"appcode": "function OnUpdate(doc, meta) {\n    var expiry = Math.round((new Date()).getTime() / 1000) + 180;\n    docTimer(timerCallback, meta.id, expiry);\n}\n\nfunction OnDelete(meta) {\n    delete dst[meta.id];}\n    \nfunction timerCallback(docid) {\n    dst[docid] = 'from timerCallback';\n}",
